{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "crust",
      "camelcase": "Crust",
      "title": "Crust",
      "scope": "source.crust",
      "file-types": [
        "crust"
      ],
      "injection-regex": "^crust$",
      "class-name": "TreeSitterCrust"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "crust syntax highlighting",
    "authors": [
      {
        "name": "Daan05"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-crust"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
